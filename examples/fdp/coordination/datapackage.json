          {
            "name": "FuncDesc",
            "type": "string",
            "format": "default",
            "title": "Functional Classification (Description)",
            "columnType": "functional-classification:generic:level1:label",
            "labelOf": "functional-classification:generic:level1:code"
          },
          {
            "name": "Func2",
            "type": "string",
            "format": "default",
            "title": "Functional Classification (Level 2)",
            "columnType": "functional-classification:generic:level2:code"
          },
          {
            "name": "Func2Desc",
            "type": "string",
            "format": "default",
            "title": "Functional Classification (Level 2) (Description)",
            "columnType": "functional-classification:generic:level2:label",
            "labelOf": "functional-classification:generic:level2:code"
          },
          {
            "name": "Committed",
            "type": "number",
            "format": "default",
            "title": "Allocated budget",
            "normalize": {
              "Phase": "Committed"
            },
            "columnType": "value-kind:code"
          },
          {
            "name": "Modified",
            "type": "number",
            "format": "default",
            "title": "Modified budget",
            "normalize": {
              "Phase": "Modified"
            },
            "columnType": "value-kind:code"
          }
        ],
        "extraFields": [
          {
            "name": "Fiscal amount",
            "type": "number",
            "columnType": "value",
            "normalizationTarget": true
          },
          {
            "name": "Currency",
            "type": "string",
            "columnType": "value-currency:code",
            "constant": "GBP"
          }
        ]
      }
    },
